{
  "name": "meta-env-typed",
  "type": "module",
  "version": "2.0.2",
  "description": "A vite plugin to generate import_meta.d.ts file for env typed.",
  "author": "JsonLee12138",
  "license": "MIT",
  "homepage": "https://github.com/JsonLee12138/meta-env-typed",
  "repository": {
    "type": "git",
    "url": "https://github.com/JsonLee12138/meta-env-typed"
  },
  "bugs": {
    "url": "https://github.com/JsonLee12138/meta-env-typed/issues",
    "email": "lijunsong2@gmail.com"
  },
  "keywords": [
    "vite",
    "rsbuild",
    "env",
    "type",
    "typed",
    "typescript",
    "env-typed",
    "env-typed-vite",
    "env-typed-rsbuild",
    "vite-plugin",
    "rsbuild-plugin"
  ],
  "exports": {
    "./vite": {
      "types": "./types/vite.d.ts",
      "import": "./dist/es/vite.mjs",
      "require": "./dist/cjs/vite.cjs"
    },
    "./rsbuild": {
      "types": "./types/rsbuild.d.ts",
      "import": "./dist/es/rsbuild.mjs",
      "require": "./dist/cjs/rsbuild.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "vite": [
        "./types/vite.d.ts"
      ],
      "rsbuild": [
        "./types/rsbuild.d.ts"
      ]
    }
  },
  "files": [
    "!dist/**/*.map",
    "!types/**/*.map",
    "CHANGELOG.md",
    "LICENSE",
    "README.en.md",
    "README.md",
    "dist",
    "types"
  ],
  "scripts": {
    "publish:beta": "npm publish --tag beta",
    "publish:release": "npm publish",
    "build": "rolldown -c rolldown.config.ts && cp -r tmpl dist/",
    "oxlint": "oxlint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint:fix": "eslint --fix .",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "@rsbuild/core": ">=1.3.22",
    "@rspack/core": ">=1.3.15",
    "vite": ">=6.0.0"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@jsonlee/tsconfig": "workspace:^",
    "@vitest/ui": "^2.0.0",
    "cp": "^0.2.0",
    "happy-dom": "^14.0.0",
    "oxlint": "^1.8.0",
    "rolldown": "1.0.0-beta.29",
    "rolldown-plugin-dts": "^0.14.1",
    "typescript": "~5.6.2",
    "vitest": "^2.0.0"
  }
}
